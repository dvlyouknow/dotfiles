-- prompt
\set PROMPT1 '%[%033[1;32m%]%M%[%033[0m%]:%[%033[1;31m%]%>%[%033[0m%] %[%033[1;36m%]%n%[%033[0m%]@%/%R%#%x '
\set PROMPT2 '%/%R %# '


\set ON_ERROR_ROLLBACK interactive
\set COMP_KEYWORD_CASE upper
\set HISTFILE ~/.psql/history
\set HISTSIZE 100000

\pset linestyle unicode
\pset border 1
\pset null 'ï³ '


\timing
\x auto
